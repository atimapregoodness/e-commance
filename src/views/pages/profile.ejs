<h2>Your Profile</h2>
<p>Welcome, <%= user.name %></p>
<h3>Your Bookings</h3>
<ul>
  <% bookings.forEach(booking => { %>
  <li>
    <%= booking.service.name %> on <%= booking.date.toDateString() %> at <%=
    booking.time %> - Status: <%= booking.status %>
  </li>
  <% if (booking.reply) { %>
  <p>Admin Reply: <%= booking.reply %></p>
  <% } %> <% }) %>
</ul>
